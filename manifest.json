{
  "manifest_version": 3,
  "name": "Notion Calendar Extension",
  "version": "1.0",
  "permissions": ["activeTab", "storage"],
  "options_ui": {
    "page": "./pages/options.html",
    "open_in_tab": true
  },
  "content_scripts": [
    {
      "matches": ["https://calendar.notion.so/**"],
      "js": ["./lib/jquery-3.7.1.min.js", "./scripts/index.js"]
    }
  ],
  "background": {
    "service_worker": "./scripts/background.js",
    "type": "module"
  }
}
